            <div class="popup__slider-container popup__content" id="design-content">
                <div class="popup__slider popup__design_images " id="design-images"></div>
            </div>
          
          <div class="popup__btn popup__btn--adjust" data-target="adjust">
            <h4 class="first_title title">Adjustments</h4>
            <svg width="65" height="30" viewBox="0 0 65 30" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M0.566243 15L15 29.4338L29.4338 15L15 0.566243L0.566243 15ZM396 12.5L15 12.5V17.5L396 17.5V12.5Z" fill="#B1ACE5"/>
            </svg>
          </div>
          {# <p class="popup__info popup__content second_info" id="adjust-content"></p> #}
          <div class="popup__slider-container popup__content" id="adjust-content">
            <div class="popup__slider popup__adjust_images " id="adjust-images"></div>
          </div>
          
        </div>
        {# 
        
        <p class="popup__info second_info"></p>   #}
      </div>

        

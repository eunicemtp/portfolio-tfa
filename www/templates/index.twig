    <section class="section section__works">
      <div class="masque"><h2 class="title" id="title">Works</h2></div>
        
      <div class="slider-container ">
        <div class="slider fixed">
          {% for key, item in projets_details %}
            <div class="slider__el" data-gradient="gradient-bg-{{ loop.index }}">
              <div class="square">
                <div class="contenu">
                  {% if loop.first %}
                    <h3 class="centered-title title--popup">{{ item.title }}</h3>
                  {% else %}
                    <h3 class="title--popup">{{ item.title }}</h3>
                  {% endif %}
                  {% for image in item.images %}
                    {% if loop.index == 2 and key == "Rux" %}
                      <img src="{{ image }}" alt="{{ item.title }}" class="second-image">
                    {% else %}
                      <img src="{{ image }}" alt="{{ item.title }}">
                    {% endif %}
                    <p class="description">{{ item.description }}</p>
                  {% endfor %}
                  {# <h3 class="title title--popup" style="display: none;">{{ item.title }}</h3> #}
                  {# <p class="description" style="display: none;">{{ item.description }}</p> #}
                  <p class="info" style="display: none;">{{ item.info }}</p>
                  <p class="info2" style="display: none;">{{ item.info2 }}</p>
                  {# <div class="design_images" style="display: none;">
                    {% for image in item.design_images %}
                      <div class="popup__slider__el"><img class="design_img" src="{{ image }}" alt="{{ item.title }}"></div>
                    {% endfor %}
                  </div> #}
                  <div class="design_images" style="display: none;">
                    <!-- Images Mobiles -->
                    {% for image in item.design_images.mobile %}
                        <div class="popup__slider__el"><img class="design_img mobile_img" src="{{ image }}" alt="{{ item.title }} Mobile Image"></div>
                    {% endfor %}
                    <div class="design_images--desktop">
                      <!-- Images Desktop -->
                      {% for image in item.design_images.desktop %}
                        <div class="popup__slider__el"><img class="design_img desktop_img" src="{{ image }}" alt="{{ item.title }} Desktop Image"></div>
                      {% endfor %}
                    </div>
                    
                  </div>
                  <div class="adjust_images" style="display: none;">
                    <!-- Images Mobiles -->
                    <div class="adjust_images">
                      {% for image in item.adjust_images.mobile %}
                          <div class="popup__slider__el">
                            <img class="adjust_img mobile_img" src="{{ image.src }}" alt="{{ item.title }} Mobile Image">
                            <p class="adjust_img--description">{{ image.description }}</p>
                          </div>
                      {% endfor %}
                    </div>
                    <div class="adjust_images adjust_images--desktop">
                      <!-- Images Desktop -->
                      {% for image in item.adjust_images.desktop %}
                        <div class="popup__slider__el">
                          <img class="adjust_img desktop_img" src="{{ image.src }}" alt="{{ item.title }} Desktop Image">
                          <p class="adjust_img--description">{{ image.description }}</p>
                        </div>
                        
                      {% endfor %}
                    </div>
                    
                  </div>
                </div>
              </div>
            </div>
          {% endfor %}
        </div>

